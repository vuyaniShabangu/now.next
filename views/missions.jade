extends layout

block content
  .page-header
    h3 View missions

  ul
    for cont in missions
      if cont.userID == user._id & cont.email!="operator"
        p="----------------------------------------------------------------------------------------------------------------------------------------------------------------------"
        | Posted On
        p=cont.mdatetime
        p="By:  "+user.email
        li=cont.mtype
        | Description:
        p=cont.mdesc

